{
  "name": "server",
  "version": "5.4.0",
  "description": "Commons administrative backend",
  "main": "index.js",
  "scripts": {
    "dev": "nodemon -e js,graphql -x babel-node --inspect src/index.js",
    "test": "NODE_ENV=test jest",
    "clear": "rimraf build",
    "build": "npm run clear && babel src --out-dir build --source-maps && npm run copy",
    "copy": "copyfiles -u 1  \"src/lib/**/*.csv\" build",
    "start": "node build/index.js",
    "deploy": "npm run build && npm run start",
    "prisma-deploy": "prisma deploy",
    "prisma-seed": "prisma seed"
  },
  "author": "",
  "license": "ISC",
  "dependencies": {
    "@cdm-logger/server": "^5.0.5",
    "amqplib": "^0.5.3",
    "apollo-server-express": "^2.8.0",
    "aws-sdk": "^2.395.0",
    "axios": "^0.19.0",
    "bcryptjs": "^2.4.3",
    "compression": "^1.7.4",
    "cookie-parser": "^1.4.4",
    "csv-parser": "^2.3.0",
    "dotenv": "^6.2.0",
    "express": "^4.17.1",
    "ffprobe-static": "^2.0.0",
    "fluent-ffmpeg": "^2.1.2",
    "fs": "0.0.1-security",
    "get-stream": "^5.1.0",
    "google-auth-library": "^2.0.2",
    "graphql": "^14.4.2",
    "graphql-import": "^0.7.1",
    "graphql-rabbitmq-subscriptions": "^1.1.0",
    "helmet": "^3.21.1",
    "jsonwebtoken": "^8.5.1",
    "jwk-to-pem": "^2.0.3",
    "lodash": "^4.17.15",
    "mammoth": "^1.4.9",
    "node-html-to-image": "^1.1.0",
    "prisma-client-lib": "^1.34.0",
    "tmp": "^0.1.0",
    "xss": "^1.0.6"
  },
  "devDependencies": {
    "@babel/cli": "^7.8.4",
    "@babel/core": "^7.2.2",
    "@babel/node": "^7.2.2",
    "@babel/preset-env": "^7.2.3",
    "@gpa-lab/eslint-config": "^1.2.0",
    "apollo-server-testing": "^2.12.0",
    "babel-eslint": "^10.0.1",
    "copyfiles": "^2.1.1",
    "eslint": "^6.8.0",
    "eslint-plugin-import": "^2.20.2",
    "eslint-plugin-jest": "^23.9.0",
    "graphql-tag": "^2.10.3",
    "jest": "^25.2.7",
    "nodemon": "^1.18.9",
    "rimraf": "^2.6.3"
  },
  "jest": {
    "coveragePathIgnorePatterns": [
      "<rootDir>/build/",
      "<rootDir>/node_modules/"
    ],
    "testPathIgnorePatterns": [
      "<rootDir>/build/",
      "<rootDir>/node_modules/"
    ]
  }
}
